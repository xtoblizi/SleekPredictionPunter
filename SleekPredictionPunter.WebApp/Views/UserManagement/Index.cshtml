@model SleekPredictionPunter.Model.PredicateForTransactionLog
@using Microsoft.AspNetCore.Http
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="row mt">
    <!-- /col-lg-12 -->
    <div class="col-lg-12 mt">
        <div class="row content-panel">
            <div class="panel-heading">
                <ul class="nav nav-tabs nav-justified">
                    <li>
                        <div style="text-align:left" data-toggle="tab">
                            <h2>Basic Profile Details</h2>
                        </div>

                        @*<div style="text-align:right" data-toggle="tab">
                            <h2>balance: &#8358;@Model.WalletModel.Amount</h2>
                        </div>*@
                    </li>
                </ul>
            </div>
            <!-- /panel-heading -->
            <div class="panel-body">
                <div class="tab-content">
                    <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12 detailed">
                                <form role="form" class="form-horizontal">
                                    <h4>
                                        <span> Subscription Details</span>
                                    </h4>
                                    <div class="form-group">
                                        <label class="col-lg-2 control-label">Current Plan Name</label>
                                        <div class="col-lg-6">
                                            <input type="text" readonly id="s-name" value="@Model?.CurrentPlan" class="form-control">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-lg-2 control-label">Expiration Date</label>
                                        <div class="col-lg-6">
                                            <input type="text" readonly id="s-exp" value="@Model?.CurrentPlanExpirationDate" class="form-control">
                                        </div>
                                    </div>

                                    <hr />
                                    <br />
                                    <h4 class="mb">Personal Information</h4>
                                    <div class="form-group">
                                        <label class="col-lg-2 control-label">First Name</label>
                                        <div class="col-lg-6">
                                            <input type="text" readonly id="c-name" value="@Model?.SubscriberModel?.FirstName" class="form-control">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-lg-2 control-label">Last Name</label>
                                        <div class="col-lg-6">
                                            <input type="text" readonly id="lives-in" value="@Model?.SubscriberModel?.LastName" class="form-control">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-lg-2 control-label">Email Address</label>
                                        <div class="col-lg-6">
                                            <input type="text" readonly id="country" value="@Model?.SubscriberModel?.Email" class="form-control">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-lg-2 control-label">Phone Contact</label>
                                        <div class="col-lg-6">
                                            <input type="text" readonly id="country" value="@Model?.SubscriberModel?.PhoneNumber" class="form-control">
                                        </div>
                                    </div>


                                </form>
                            </div>
                        </div>
                    </div>
                    <!-- /tab-pane -->
                </div>
                <!-- /tab-content -->
            </div>

            <!-- /panel-body -->
        </div>
