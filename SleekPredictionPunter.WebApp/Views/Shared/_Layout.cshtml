	<meta http-equiv="X-UA-Compatible" content="ie=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>@ViewData["Title"] - Predictive Power </title>

	@*@if (ViewBag.AddLinkScriptforPackage != null && ViewBag.AddLinkScriptforPackage == true)
